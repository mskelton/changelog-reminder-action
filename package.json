{
  "name": "changelog-reminder-action",
  "version": "0.1.0",
  "private": true,
  "description": "Github action to remind users their pull request is missing a changelog.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mskelton/changelog-reminder-action.git"
  },
  "keywords": [
    "actions",
    "changelog",
    "reminder"
  ],
  "author": "Mark Skelton",
  "license": "ISC",
  "packageManager": "yarn@3.3.1",
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module",
  "exports": "./lib/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "build": "ncc build",
    "test": "node --experimental-vm-modules $(yarn bin jest)"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@mskelton/eslint-config": "^7.1.0",
    "@mskelton/tsconfig": "^2.0.0",
    "@types/jest": "^29.2.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^8.32.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-sort": "^2.4.0",
    "jest": "^29.4.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4"
  }
}
